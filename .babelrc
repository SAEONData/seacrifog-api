{
  "presets": [[
    "@babel/preset-env",
    {
      "useBuiltIns": "usage",
      "corejs": 3,
      "targets": {
        "node": "13"
      },
      "modules": "commonjs"
    }
  ]],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    [
      "@babel/plugin-transform-runtime",
      {"corejs": {"version": 3, "proposals": true}, "regenerator": true}
    ]
  ]
}